{
  "name": "vue-clean-admin",
  "version": "1.0.0",
  "description": "基于Vue3、Vite、TypeScript、Element-Plus的精简中后台管理系统",
  "author": "Bryan Qiu(十五)",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite -c build/vite.config.ts",
    "lint": "pnpm lint:eslint && pnpm lint:types && pnpm lint:style",
    "lint:eslint": "eslint . --cache --max-warnings 0 --cache-location node_modules/.cache/eslint",
    "lint:types": "vue-tsc --noEmit --pretty false",
    "lint:style": "stylelint \"**/*.{css,scss,sass,vue,html}\" --cache --cache-location node_modules/.cache/stylelint --max-warnings 0",
    "format": "prettier --write --cache \"**/*.{js,ts,jsx,tsx,vue,css,scss,sass,html,json,md,yml,yaml}\"",
    "format:check": "prettier --check --cache \"**/*.{js,ts,jsx,tsx,vue,css,scss,sass,html,json,md,yml,yaml}\"",
    "build": "vite build --mode production -c build/vite.config.ts",
    "preview": "vite preview -c build/vite.config.ts",
    "cache:clear": "rimraf node_modules/.cache node_modules/.vite .eslintcache .stylelintcache .prettier-cache",
    "prepare": "husky",
    "commit": "git-cz",
    "view:eslint": "eslint-config-inspector --config eslint.config.js",
    "vite": "vite --debug transform -c build/vite.config.ts"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@vueuse/core": "^11.0.1",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "element-plus": "^2.13.1",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "numbro": "^2.5.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "plus-pro-components": "^0.1.30",
    "qs": "^6.14.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.16",
    "vue-i18n": "11",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@eslint/config-inspector": "^0.5.4",
    "@eslint/js": "^9.9.1",
    "@iconify/types": "github:iconify/types",
    "@iconify/vue": "^4.1.2",
    "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
    "@plus-pro-components/resolver": "^0.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.4.1",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.18",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "eslint": "^9.9.1",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.2.9",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.1",
    "postcss-scss": "^4.0.9",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "1.85.1",
    "stylelint": "^17.0.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.2.0",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-turbo-console": "^1.10.1",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^5.4.3",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.4.4",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.29"
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "admin",
    "vue admin",
    "vue3 admin",
    "vite admin",
    "clean admin",
    "vue clean admin",
    "typescript",
    "element plus"
  ],
  "engines": {
    "node": ">=18.20.8"
  },
  "homepage": "https://github.com/bryqiu/vue-clean-admin",
  "repository": "https://github.com/bryqiu/vue-clean-admin",
  "bugs": {
    "url": "https://github.com/bryqiu/vue-clean-admin/issues"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
